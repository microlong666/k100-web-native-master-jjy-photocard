{
  "name": "k100-web-native",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run serve:vue\"",
    "build": "webpack --config ./build/webpack.prod.conf.js",
    "serve:vue": "vue-cli-service serve",
    "build:vue": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "dev:component": "webpack --config ./build/webpack.dev.conf.js",
    "build:component": "webpack --config ./build/webpack.prod.conf.js",
    "dev:server": "node ./server/server.js"
  },
  "dependencies": {
    "core-js": "^3.4.4",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cors": "^2.8.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "socket.io": "^2.3.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-cli": "^3.3.10",
    "zip-webpack-plugin": "^3.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
